{
    "version": 2,
    " build": [
    {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "dist/tailwind-learn/browser"
        }
    }
],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}